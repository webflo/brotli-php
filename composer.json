{
    "name": "hellonico/brotli",
    "type": "library",
    "description": "Add brotli compress/uncompress functions to PHP. Batteries included.",
    "keywords": [
        "brotli"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Lemoine",
            "email": "tech@niconico.fr"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "loophp/phposinfo": "^1.7",
        "symfony/process": "^4.4 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "HelloNico\\Brotli\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --testdox"
        ]
    }
}
